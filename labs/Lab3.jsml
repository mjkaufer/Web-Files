var title = "Name the Capitals";

[

	{
		t: "head",
		c:[
			{
				t:"link",
				a:{
					"rel":"stylesheet",
					"type":"text/css",
					"href":"css/style.css"
				}
			},
			{
				t:"title",
				T: title
			}
		]
	},
	{
		t: "body",
		c:[
			{
				t:"h1",
				T: title
			},
			{
				t:"p",
				T:"Time - ",
				c:{
					t:"span",
					T:"0:05",
					a:{
						id:"time"
					}
				}
			},
			{
				t:"p",
				T:"Remaining States - ",
				c:{
					t:"span",
					T:"50",
					a:{
						id:"remaining"
					}
				}
			},
			{
				t:"input",
				a:{
					type:"text",
					id:"input",
					autofocus:"true"
				},
			},
			{
				t:"button",
				T:"Submi(t)",
				a:{
					id:"next",
					accesskey:"t"
				}
			},
			{
				t:"br",
			},
			{
				t:"button",
				T:"(S)tart",
				a:{
					id:"start",
					accesskey:"s"
				}
			},
			{
				t:"p",
				T:"",
				a:{
					id:"message"
				}
			},
			{
				t:"script",
				a:{
					src:"Lab3.js"
				}
			}

		]
	}


]