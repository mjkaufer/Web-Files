var labCount = 9;

var labs = {
	t:"ul",
	c:[
		{
			t:"li",
			c:[
				{
					t:"a",
					T:"Home Page",
					a:{
						href:"./"
					}
				},
				{
					t:"a",
					T:"(src)",
					a:{
						href:"./index.jsml"
					}
				}
			]
		}
	]
}
var miscFiles = ["Wastebin"];

var phpLabs = [6, 7, 8, "Wastebin", 9];

for(var i = 1; i <= labCount; i++){
	var li = {
		t:"li",
		c:[]
	};

	var isPHP = phpLabs.indexOf(i) >= 0;

	var suffix = isPHP ? ".php" : ".html";
	var sourceSuffix = isPHP ? ".phps" : ".jsml";

	li.c.push({
		t:"a",
		T:"Lab " + i,
		a:{
			href:"./labs/Lab" + i + suffix
		}
	});

	li.c.push({
		t:"a",
		T:"(src)",
		a:{
			href:"./labs/Lab" + i + sourceSuffix
		}
	});

	labs.c.push(li)

}


var li = {
	t:"li",
	c:[]
};

li.c.push({
	t:"a",
