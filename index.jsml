labs.c.push(li)



labs.c.push({t:"hr"});

for(var index in miscFiles){
	var i = miscFiles[index]
	var li = {
		t:"li",
		c:[]
	};

	var isPHP = phpLabs.indexOf(i);

	var suffix = isPHP ? ".php" : ".html";
	var sourceSuffix = isPHP ? ".phps" : ".jsml";

	li.c.push({
		t:"a",
		T:"Misc Lab: " + i,
		a:{
			href:"./labs/" + i + suffix
		}
	});

	li.c.push({
		t:"a",
		T:"(src)",
		a:{
			href:"./labs/" + i + sourceSuffix
		}
	});
	
	labs.c.push(li)

}

